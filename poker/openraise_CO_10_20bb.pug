							th
								a(href="openraise_UTG_10_20bb.html") UTG
							th("colspan"=2)
								a(href="openraise_UTG1_10_20bb.html") UTG+1
							th("colspan"=2)
								a(href="openraise_UTG2_10_20bb.html") UTG+2
